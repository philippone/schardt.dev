---
layout: layouts/post-overview.njk
eleventyNavigation:
  key: projects
  order: 2
---

<h2>Projects</h2>

{% for post in collections.project | reverse | localization(locale) %}
    {% if loop.first %}
        <ul>
        {% endif %}
        <li>
            <article>
            {{ post.url }}
                <p>
                    <time datetime="{{ post.date | date('Y-MM-DD') }}">{{ post.date | date("DD MMMM[,] Y", locale) }}</time>
                </p>
                <h3>
                    <a href="{{ post.url }}">{{ post.title }} hi</a>
                </h3>
            </article>

        </li>

        {% if loop.last %}
        </ul>
    {% endif %}
{% endfor %}